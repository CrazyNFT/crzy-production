{
  "hosting": {
    "public": "out",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/marketplace",
        "destination": "/marketplace.html"
      },
      {
        "source": "/comingsoon",
        "destination": "/comingsoon.html"
      },
      {
        "source": "/create",
